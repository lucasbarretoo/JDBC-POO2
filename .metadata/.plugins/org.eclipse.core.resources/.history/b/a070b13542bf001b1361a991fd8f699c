public class db {
	
	
	private static Connection Conexao;
	
	public static Properties getPropriedades() {
		Properties propriedade = new Properties();
		FileInputStream file = new FileInputStream("./propriedades/db.properties");
		propriedaes.load(file);
	}
	
}
